{
  "name": "@siteboon/n8n-nodes-siteboon",
  "version": "0.1.2",
  "description": "n8n community node for Siteboon AI website builder API",
  "license": "MIT",
  "homepage": "https://siteboon.ai",
  "keywords": [
    "n8n-community-node-package",
    "siteboon",
    "website-builder",
    "ai",
    "automation",
    "n8n",
    "n8n-nodes",
    "siteboon-node"
  ],
  "author": {
    "name": "Simos",
    "email": "simos@siteboon.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/siteboon/n8n-nodes-siteboon.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
